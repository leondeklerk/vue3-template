{
  "name": "vue3-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bun run lint && bun run type-check && bun run build-only",
    "build-only": "bunx --bun vite build",
    "lint": "eslint --ext .ts,.vue,.js --max-warnings 0 .",
    "lint:fix": "eslint --ext .ts,.vue,.js --fix .",
    "prepare": "husky",
    "preview": "bunx --bun vite preview",
    "test:unit": "bunx --bun vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false "
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --ext .ts,.vue,.js --max-warnings 0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.6.0",
    "pinia": "2.2.1",
    "vue": "3.4.36",
    "vue-router": "4.4.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.4",
    "@tailwindcss/forms": "0.5.7",
    "@tsconfig/node20": "20.1.4",
    "@types/bun": "1.1.6",
    "@vitejs/plugin-vue": "5.1.2",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.27.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.8",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.8",
    "typescript": "5.5.4",
    "vite": "5.4.0",
    "vitest": "1.6.0",
    "vue-tsc": "2.0.29"
  }
}
